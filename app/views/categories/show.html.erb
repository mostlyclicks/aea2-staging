<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @category.name %>
</p>

<p>
  <b>Description:</b>
  <%= @category.description %>
</p>

<ul>
  <% @category.assets.each do |ca| %>
    <li>
    <h3><%= ca.name %></h3>
    <p><%= ca.description %></p>
    </li>
    <% end %>
</ul>

<%= link_to 'Edit', edit_category_path(@category) %> |
<%= link_to 'Back', categories_path %>
